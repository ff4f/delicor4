<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for product.request -->
        <record id="seq_box_in_req" model="ir.sequence">
            <field name="name">Cash Box IN</field>
            <field name="code">cash.box.in</field>
            <field name="prefix">IN/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_box_out_req" model="ir.sequence">
            <field name="name">Cash Box OUT</field>
            <field name="code">cash.box.out</field>
            <field name="prefix">SAL/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>